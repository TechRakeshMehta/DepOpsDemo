.layout_wrapper
{
    width: 90%;
    margin: 0 auto;
}

.zones_left
{
    float: left;
    width: 39.9%;
    border: 0;
    margin: 0;
    padding: 0;
}

.zones_right
{
    float: left;
    width: 59.9%;    
    border: 0;
    margin: 0;
    padding: 0;
}

.zones_right_wrap
{
}

.zone_col_wrap
{
    float: left; width: 33.3%; vertical-align: top; 
}

