}

.zone
{
    border-style:none!important;
    padding:0 4px!important;
    min-height:0!important;
}

.visibleZone
{
    border-style:dashed!important;
    padding:4px!important;
    min-height:10px !important;
    margin:10px 0;
    background-color:aliceblue;
}