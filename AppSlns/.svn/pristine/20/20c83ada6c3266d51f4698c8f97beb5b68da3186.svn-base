html{width: 100%;height: 100%;}
form{width: 100%;height: 100%;}
body{width: 100%;padding: 0;margin: 0;font-size:12px;font-family: Verdana,arial;overflow: auto;height:100%;background: #fff repeat-x;}

/*General Classes*/
.content-wrapper{width:1024px;margin:0 auto;position:relative;}
.no-box{margin:0;padding:0}
span.highlight{font-weight:bold;}
.reset{clear:both;}
.no-list-style {list-style:none;}
.gap-10H {margin-bottom:10px;}
.gap-5H {margin-bottom:5px;}


/*Page wrapper & footer*/
.page-wrapper{ min-height: 100%;height: auto !important;height: 100%;margin: 0 auto -87px;min-width:1024px;}
.footer-wrapper{height:77px;padding:5px;border-top:1px;min-width:1024px;}
.push {height: 90px;}

/*Header*/
.hdr-wrap{background-image:url(images/hdrbkg_Red.png);background-repeat:repeat-x;height:110px;padding-top:1px;}
h1.header {color:#E8EDE0;text-shadow:1px 1px 4px black;font-size:22px;background-image:url(images/complioBlue.png);background-repeat:no-repeat;height:80px;margin-top:24px;}
.poweredby {position:absolute; top:24px; right:0px;background-image:url(images/powered.png);background-repeat:no-repeat;width:85px;height:63px;display:none;}

/*footer*/
.footer-wrapper {background: url(images/foot_Red.png) repeat-x;}
.dynamic {float:left;color:#4880bf;margin-top:23px;}
.dynamic a {color:#4880bf;} .dynamic a:hover {color:#4880bf;}
.static {float:right;font-size:11px;color:#4880bf;margin-top:6px;}

/*Client Logo*/
.logo{margin:30px 0;margin-bottom:100px;}
.logo img {max-height:100px}
.client-logo{height:70px;margin:20px 0;}
.client-logo img{height:70px;}

/*Command bar*/
.c .sxcmds{ padding:3px;text-align: center;}
.l .sxcmds{ padding:3px;text-align: left;}
.r .sxcmds{ padding:3px;text-align: right;}

/*Typography*/
h1.page_header {border-bottom: 1px solid;color: #8C1921;font-family: Arial;font-size: 20px;}


.cptn:after { content:':';}