/*===>===>===>===>===>===>===>===>===>===>===>
Defining style for core html elements*/

/*Reseting broswer elements*/
html {
    color: #000;
}

body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, button, textarea, select, p, blockquote, th, td {
    margin: 0;
    padding: 0;
}

table {
    border-collapse: collapse;
    border-spacing: 0;
}

fieldset, img {
    border: 0;
}

address, button, caption, cite, code, dfn, em, input, optgroup, option, select, strong, textarea, th, var {
    font: inherit;
}

del, ins {
    text-decoration: none;
}

li {
    list-style: none;
}

caption, th {
    text-align: left;
}

h1, h2, h3, h4, h5, h6 {
    font-size: 100%;
    font-weight: normal;
}

q:before, q:after {
    content: '';
}

abbr, acronym {
    border: 0;
    font-variant: normal;
}

sup {
    vertical-align: super;
}

sub {
    vertical-align: sub;
}

legend {
    color: #000;
}

/*Setting base html element*/
html {
    height: 100%;
    overflow: hidden!important;
    font-size: 100%;
}

/*Setting base body element*/
body {
    width: 100%;
    height: 100%;
    font-family: Helvetia, Arial, sans-serif!important;
    font-size: 100% !important;
    margin: 0;
    padding: 0;
}

/*Setting default border styles for all elements*/
* {
    border-style: solid;
    border-width: 0;
    border-color: #adadad;
}


