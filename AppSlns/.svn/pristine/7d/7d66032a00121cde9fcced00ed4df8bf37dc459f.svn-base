/*--->--->--->--->--->--->--->--->--->--->--->
Defining style for Sections and Tabbed views*/

/*Classes that represent the Outer Container for Section and tabbed views*/
/*Defining base styles for both the views*/
.section, .tabvw {
    padding: 10px;
    margin-bottom: 10px;
}

.section {
}

    /*A class representing the main header element of the section*/
    .section .mhdr {
        padding: 0 0 8px 0;
        margin: 0 !important;
        font-size: 133% !important;
        cursor: pointer;
    }

    /*A class that represents the container for section contents*/
    .section .content {
        margin: 0 0 10px;
        overflow-x: auto;
        overflow-y: hidden;
        position: relative;
    }

        /*A class that represents the sub header element of the section that should be placed inside content*/
        .section .content .shdr {
            font-size: 100% !important;
            margin: 8px 0 8px 0px!important;
        }

/*A class that defines the styles for tab strip controls*/
.tabstrp {
}

/*Add more styles for tabbed views*/
.tabvw {
    border-bottom-width: 1px;
    border-right-width: 1px;
    border-left-width: 1px;
    padding-top: 20px;
}

    /*A class that reprenting section inside a tabbed view*/
    /*Overdding styles of section class as defined above*/
    .tabvw .section {
        margin: 0 !important;
        padding: 0 !important;
    }

        /*Overdding styles of main header class as defined above*/
        .tabvw .section .mhdr {
            font-size: 113% !important;
            padding: 0 0 0px;
        }

        /*Overdding styles of section content class as defined above*/
        .tabvw .section .content {
            margin: 8px 0 4px;
        }

    /*Overdding styles of the collapsed header class as defined above*/
    .tabvw .colps {
        background-position: -4px 0;
    }

/*Surender - style to fix font size in tab view*/
.tabw .section .mhdr {
    font-size: 120% !important;
}

