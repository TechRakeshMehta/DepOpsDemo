<div class="container-fluid" id="bodyBlock">
  <section class="p-2">
    <div class="row">
      <div class="col-md-12 text-center">
        <h3 id="CRTACCOUNT"></h3>
      </div>
      <div class="col-md-12 bg-white shadow-sm rounded py-3">
        <div class="custom-control custom-checkbox mr-sm-2">
          <input class="custom-control-input" type="checkbox" [disabled]="IsAliasCheckboxDisable" [(ngModel)]="IsAliasCheckboxChecked" (change)="AddAlias($event)" id="maiden1">
          <label class="custom-control-label" for="maiden1"><label id="ISALIASNAME"></label></label>
        </div>
        <alias-info (Next)="onNext($event)" (ShowHideAddMoreButton)="onShowHideAddMoreButton($event)" *ngFor="let alias of lstAlias" [alias]="alias"></alias-info>
        <button [hidden]="hdnAddMoreAliasbtn" type="button" (click)="AddMoreAlias()" class="btn btn-primary">
          <i class="fas fa-plus-circle mr-1"></i><label id="ADDMORE" class="mb-0"></label></button>
        <div class="row mt-1">
          <div class="col-md-12 d-flex justify-content-center">
            <button type="button" (click)="onBack()" class="btn btn-primary fixedWidthBtn mr-1">
              <i class="fas fa-arrow-left pr-1"></i><label id="PREVIOUS"></label></button>
            <button type="button" [disabled]="IsNextButtonDisabled" (click)="onSubmit()" class="btn btn-primary fixedWidthBtn mr-1"><label id="NEXT"></label>
              <i class="fas fa-arrow-right pl-1"></i>
            </button>
            <cancel-registration-button></cancel-registration-button>
          </div>
        </div>
      </div>
    </div>
  </section>
</div>