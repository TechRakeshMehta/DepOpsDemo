                        <%--   <div id="dvSharedUserBreadcrumb" runat="server" visible="false" class="container-fluid">
                            <div class="row">
                                <div class="col-md-12">
                                   <h1 class="text-right" style="overflow:hidden; height:32px;">
                                        <asp:Label Text="" runat="server" ID="lblModHdrSharedUser" />
                                        <%= this.Title %>
                                        <asp:Label Text="" runat="server"
                                            ID="lblPageHdrSharedUser" CssClass="phdr" /></h1>
                                    <div class="breadcrumb padbott10 text-right" id="dvBreadcrumbControl" runat="server">
                                        <infsu:breadcrumb ID="ucSharedUserBreadcrumb" runat="server" />
                                    </div>
                                </div>
                            </div>
                        </div>--%>
                    </ContentTemplate>
                </asp:UpdatePanel>
            </asp:Panel>
            <%-- Error / Information Box --%>
            <asp:UpdatePanel ID="pnlError" runat="server" UpdateMode="Conditional">
                <ContentTemplate>
                    <div class="msgbox" id="pageMsgBox">
                        <asp:Label CssClass="info" EnableViewState="false" runat="server" ID="lblError"></asp:Label>
                    </div>
                </ContentTemplate>
            </asp:UpdatePanel>
            <%-- Main content / Body of the page --%>
            <asp:UpdatePanel ID="UpdatePanel1" runat="server" UpdateMode="Always">
                <ContentTemplate>

                    <div id="box_content">
                        <asp:ContentPlaceHolder ID="DefaultContent" runat="server" />
                        <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server" />
                        <asp:PlaceHolder runat="server" ID="plcDynamic"></asp:PlaceHolder>
                    </div>
                </ContentTemplate>
            </asp:UpdatePanel>
        </div>
        <infs:WclToolTipManager runat="server" EnableTheming="false" AutoTooltipify="false"
            ID="tlpmPage" Visible="false" />
        <!-- Rendered Block End!-->
        <infs:WclResourceManagerProxy runat="server" ID="prxMP">
            <infs:LinkedResource Path="~/Resources/Mod/Dashboard/Styles/kendo.default.min.css" ResourceType="StyleSheet" />
            <infs:LinkedResource Path="~/Resources/Mod/Dashboard/Styles/kendo.common.min.css" ResourceType="StyleSheet" />
            <infs:LinkedResource Path="~/Resources/generic/page.min.js" ResourceType="JavaScript" />
            <infs:LinkedResource Path="~/Resources/Mod/Dashboard/Scripts/Kendo/kendo.all.min.js" ResourceType="JavaScript" />
            <infs:LinkedResource Path="~/Resources/Mod/Shared/livehelp.css" ResourceType="StyleSheet" />
        </infs:WclResourceManagerProxy>
        <div runat="server" id="lhnScript" visible="false">
            <script type="text/javascript">
                var lhnAccountN = "19766";
                var lhnWindowN = 0;
                var lhnDepartmentN = 0;
                var lhnHPMoreOptions = false;
                var lhnHPCallbackButton = false;
            </script>
            <script src="//www.livehelpnow.net/lhn/widgets/helpouttab/lhnhelpouttab-current.min.js"
                type="text/javascript" id="lhnscriptho"></script>
        </div>
        <asp:HiddenField ID="hdntimeout" runat="server"></asp:HiddenField>
        <asp:HiddenField ID="hdnSuccessText_LangCulture" runat="server" Value="<%$Resources:Language,SUCCESSTEXT %>" ></asp:HiddenField>
        <asp:HiddenField ID="hdnCloseButtonText_LangCulture" runat="server" Value="<%$Resources:Language,CLOSE %>" ></asp:HiddenField>
    </form>
</body>



<script type="text/javascript">

    function SetPageCountDown() {

        if (parent.StartCountDown != undefined) {
            parent.StartCountDown('<%= TimeoutMinutes %>')
        }
    }
    function openURLInPageFrame(url) {
        var outeranchor = $jQuery('#outeranchor', $jQuery(window.parent.document));
        outeranchor.attr('href', url);
        outeranchor[0].click();
    }
    function openInPageFrame(sender) {

        var url = sender.href;
        if (url == null || url == '') {
            return true;
        }
        if ($jQuery(sender).is('[disabled=disabled]')) {
            return true;
        }
            //sender.removeAttribute("href");
        else {

            openURLInPageFrame(url);
            return false;
        }
    }
</script>
</html>
