                WindowCheckFailUrl="Default.aspx" EnableWindowCheck="true" IFrameMode="true"
                SupportedBrowsers="ie7,ie8,ie9" SupportedEngines="trident" EnableEngineSupport="true" EnablejQueryByDefault="true" />

