
export class CustomDayStyle {
    public date: Date;
    public mode: string;
    public clazz: string;

    constructor(date: Date, mode: string, clazz: string) {
        this.date = date;
        this.mode = mode;
        this.clazz = clazz;
    }
}

export class CustomDateStyle {
    public year: number;
    public month: number;
    public day: number;


    constructor(year: number, month: number, day: number) {
        this.day = day;
        this.month = month;
        this.year = year;
    }
}

export class HighLightDates {
    public highLightDateList: Array<CustomDayStyle>;
}

export class HighLightDateRange {
    public highLightDateRange: Array<CustomDateStyle>;
}