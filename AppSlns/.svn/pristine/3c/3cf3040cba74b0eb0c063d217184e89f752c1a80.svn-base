<script type="text/javascript">
        $jQuery(document).ready(function () {

        var profileSharingTimer = parseInt("<%= ProfileSharingInvitationConfirmationTimerInterval%>");
        var minusOne = parseInt("<%= INTSOF.Utils.AppConsts.MINUS_ONE%>");
        var isCurrentUserIsAdminOrClientAdmin = "<%=IsCurrentUserIsAdminOrClientAdmin %>";

        $jQuery(".msgbox .msg_cmd").first().click(function () {

            $jQuery(this).parents(".msgbox:last").fadeOut();
        });
        $jQuery(".reg-complete").each(function () {
            if ($jQuery(this).text().replace(/\s/g, "") != "") {
                $jQuery(this).parents(".msgbox:last").fadeIn();
            }
            });

        $jQuery(document).ready(function () {
            if (isCurrentUserIsAdminOrClientAdmin.toLowerCase() == "true") {
                if (profileSharingTimer != minusOne) {
                    //Set Time Interval
                    setInterval(function () {
                        var isNeedToStartPolling = $jQuery('[id$=hdnStartPolling]').val(); 
                        if (isNeedToStartPolling.toLowerCase() == "true") {
                            ProfileSharingConfirmation.GetProfileSharingInvitationConfirmation();
                        }
                    }, profileSharingTimer);
                }
            }
            });

            $jQuery(document).ready(function () {
            if (isCurrentUserIsAdminOrClientAdmin.toLowerCase() == "true") {
                if (profileSharingTimer != minusOne) {

                    ProfileSharingConfirmation.IsNeedToStartPolling();

                    //Set Time Interval
                    setInterval(function () {
                        var isNeedToStartPolling = $jQuery('[id$=hdnStartPolling]').val(); 
                        if (isNeedToStartPolling.toLowerCase() == "true") {
                            ProfileSharingConfirmation.IsNeedToStartPolling();
                        }
                    }, profileSharingTimer*2);
                }
            }
        });

        $jQuery("[id$=ddlTenantName_Input]").attr('role', 'combobox');
        $jQuery("[id$=clientAdminddlTenantName_Input]").attr('role', 'combobox');

        });
    //$jQuery(document).ready(function () {
    //    $jQuery("[id$=dvChangePassword]").hover(function () {
    //        $jQuery("[id$=divChangePassword]").fadeToggle();
    //    });
    //});

</script>
