    <script type="text/javascript">

        var rotationCustomAttributeWindow = null;

        function HandleDashboardCommandNavigation(navUrl, referer) {
            //set dashboard cmd navigation
            if (referer == "VideoLink") {
                //UAT-1027 : Update video tutorial link to open external page in a new browser tab. 
                var win = window.open(navUrl, '_blank');
                if (win) {
                    //Browser has allowed it to be opened
                    win.focus();
                }
            }
            else {
                // $jQuery("#ifrPage").attr('src', navUrl);
                var outeranchor = $jQuery('#outeranchor', $jQuery(window.parent.document));
                outeranchor.attr('href', navUrl);
                outeranchor[0].click();
            }
        }

        function pageLoad() {

            //Handle Shared User Navigation width.
            var width = "";
            var numOfDiv = $jQuery("#navUl").children("li").length;
            switch (numOfDiv) {
                case 1:
                    width = "0%";
                    break;
                case 1:
                    width = "100%";
                    break;
                case 2:
                    width = "50%";
                    break;
                case 3:
                    width = "33.33%";
                    break;
                case 4:
                    width = "25%";
                    break;
                case 5:
                    width = "20%";
                    break;
                case 6:
                    width = "16.66%";
                    break;
                case 7:
                    width = "14.28%";
                    break;
                case 8:
                    width = "12.5%";
                    break;
                case 9:
                    width = "11.11%";
                    break;
                default:
                    width = "1%";

            }
            $jQuery("#navUl").children("li").css("width", width);
        }

        function Side_VideoTutorialNavigation() {
            var screenname = "Get Started Video";
            var composeScreenWindowName = "Get Started Video";
            var url = $page.url.create("~/ProfileSharing/Pages/GetStartedVideo.aspx?ScreenName=" + screenname);
            //UAT-2364
            var popupHeight = $jQuery(window).height() * (80 / 100);

            var win = $window.createPopup(url, { size: "800," + popupHeight, behaviors: Telerik.Web.UI.WindowBehaviors.Close | Telerik.Web.UI.WindowBehaviors.Move, name: composeScreenWindowName });
        }

        function CallDummyPostBack(sender) {
            // $jQuery("[id$=btnDummyPostBack]").click();
            $jQuery.ajax({
                type: "POST",
                url: "/Default.aspx/ClearBreadCrumb",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data) {
                }
            });

        }
        //UAT-2003: Add ability to extend/renew when clicking "place order".
        function OpenRenewOrderPopup() {

            var url = $page.url.create("~/ComplianceOperations/Pages/RenewOrderPopup.aspx?IsFromRenewOrderPopup=true");
            var title = "Renew Existing Tracking Subscription";
            //UAT-2364
            var popupHeight = $jQuery(window).height() * (80 / 100);

            var win = $window.createPopup(url,
                {
                    size: "800," + popupHeight,
                    behaviors: Telerik.Web.UI.WindowBehaviors.Close | Telerik.Web.UI.WindowBehaviors.Maximize | Telerik.Web.UI.WindowBehaviors.Move | Telerik.Web.UI.WindowBehaviors.Modal, onclose: onCloseRenewOrderPopup
                },
                function () {
                    this.set_title("American Databank | " + title);
                    this.set_destroyOnClose(true);
                    this.set_status("");
                });

        }

        function onCloseRenewOrderPopup(oWnd, args) {
            oWnd.remove_close(onCloseRenewOrderPopup);
            if (args != undefined && args != null && args._argument != undefined && args._argument != null) {
                var outeranchor = $jQuery('#outeranchor', $jQuery(window.parent.document));
                outeranchor.attr('href', args._argument.Url);
                outeranchor[0].click();
                // $jQuery("#ifrPage").attr('src', args._argument.Url);
            }
        }

        function DisplayPlaceOrder(mode) {
            $jQuery("#PlaceNewOrder").css("display", mode);
        }

        //UAT-2251: Ability to turn on/off the initial student videos individually and to replace the video link by institution
        function OverrideVideoUrl(NavURlForVideoTut, OverrideVideoURL) {
            //   debugger;
            var hdnOverrideVideoURL = OverrideVideoURL;
            var EncrptedURLs = btoa(hdnOverrideVideoURL);
            var win = window.open(NavURlForVideoTut + "?args=" + EncrptedURLs, '_blank');
            if (win) {
                //Browser has allowed it to be opened
                win.focus();
            } else {
                //Broswer has blocked it
                //alert('Please allow popups for this site');
            }
        }

        function closeCustomAttributePopUp(isCustomFilterApplied) {
            var oArg = {};
            $jQuery("#hdnIsCustomFieldValue").val(isCustomFilterApplied);
            rotationCustomAttributeWindow.Close(oArg);
        }

        function setIsCustomAttributeApplied(isCustomFilterApplied) {
            $jQuery("#hdnIsCustomFieldValue").val(isCustomFilterApplied);
        }

