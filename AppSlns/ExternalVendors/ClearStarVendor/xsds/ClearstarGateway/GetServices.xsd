                    <xs:element name="sDescrip" type="xs:string" minOccurs="0" />
                    <xs:element name="sCharge" type="xs:string" minOccurs="0" />
                    <xs:element name="sComponentLimit" type="xs:string" minOccurs="0" />
                    <xs:element name="sComponentCharge" type="xs:string" minOccurs="0" />
                    <xs:element name="sComponentExceedCharge" type="xs:string" minOccurs="0" />
