<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="ExternalVendors.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <applicationSettings>
    <!-- AMS logging -->
    <!-- [true, false] -->
    <add key="EnableLogging" value="true" />
    <!-- choose levels to log-->
    <add key="LogDebug" value="true" />
    <add key="LogInfo" value="true" />
    <add key="LogError" value="true" />
    <add key="LogUIError" value="true" />
    <add key="LogPerf" value="true" />
    <add key="LogSource" value="ADB Clearstar Gateway" />
    <add key="LogToLogServer" value="false" />
    <add key="LogServerConnection" value="Data Source=jupiter;Initial Catalog=AMS;Persist Security Info=True;User ID=ADB_Contractor;Password=fructose1;" />
    <ExternalVendors.Properties.Settings>
      <setting name="ExternalVendors_ClearstarGateway_Profile" serializeAs="String">
        <value>https://gateway.clearstar.net/CSGWProfile/Profile.asmx</value>
      </setting>
      <setting name="ExternalVendors_ClearstarGatewayOrder_Order" serializeAs="String">
        <value>https://gateway.clearstar.net/CSGWOrder/Order.asmx</value>
      </setting>
      <setting name="ExternalVendors_ClearstarGatewayInvoice_Invoice" serializeAs="String">
        <value>https://gateway.clearstar.net/CSGWInvoice/Invoice.asmx</value>
      </setting>
      <setting name="ExternalVendors_ClearstarGatewayLookup_LookUp" serializeAs="String">
        <value>https://gateway.clearstar.net/CSGWLookup/Lookup.asmx</value>
      </setting>
      <setting name="ExternalVendors_ClearstarGatewayDocumentService_Document" serializeAs="String">
        <value>https://gateway.clearstar.net/CSGWDocument/Document.asmx</value>
      </setting>
      <setting name="ExternalVendors_ClearstarGatewayService_Document" serializeAs="String">
        <value>https://gateway.clearstar.net/CSGWService/Service.asmx</value>
      </setting>
    </ExternalVendors.Properties.Settings>
  </applicationSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework" />
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
</configuration>