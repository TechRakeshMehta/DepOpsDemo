<%@ Control Language="C#" AutoEventWireup="true" CodeBehind="AMSApplicantLandingPage.ascx.cs" Inherits="CoreWeb.Main.AMSApplicantLandingPage" %>
<%@ Register TagPrefix="infs" Namespace="INTERSOFT.WEB.UI.WebControls" Assembly="INTERSOFT.WEB.UI" %>
<%@ Register TagPrefix="telerik" Namespace="Telerik.Web.UI" Assembly="Telerik.Web.UI" %>
<div style="width: 80%; margin: 0 auto; margin-top: 50px">
    <h1>
        <asp:Label ID="lblFirstTimeLoginMessage" runat="server" Text="Thank you for registering"></asp:Label></h1>
    <div style="font-size: 110%; line-height: 150%; margin-top: 30px">
        <p>
            Welcome to the AMS community.<%-- Your account is now setup, and you are ready to
            place your order! Just click on the <span style="font-weight: bold">Order Subscription</span>
            button below to get started.
        </p>
        <p>
            AMS will guide you through a series of questions about your educational experience.
            Your responses will determine your ADB account settings.
        </p>
        <p>
            Once you login, your AMS account will walk you through the process required
            for you to be COMPLIANT. Help is always available – just use the Communication Center
            at the top left of each screen. Or, you can call us at (800) 200-0853.--%>
        </p>
    </div>
    <div style="padding-top: 30px; text-align: right">
        <infs:WclButton Font-Bold="true" Height="30px" BackColor="Transparent" ButtonType="LinkButton"
            runat="server" ID="btnPendingOrder" Text="&nbsp;Order Subscription&nbsp;">
        </infs:WclButton>
        &nbsp;&nbsp;
        <infs:WclButton Height="30px" BackColor="Transparent" ButtonType="LinkButton" runat="server"
            ID="btnDashBoard" Text="&nbsp;Go to Dashboard&nbsp;">
        </infs:WclButton>
    </div>
</div>

