    function OpenExistingOrderPopUpScreen(openExistingOrderPopup) {
        if (openExistingOrderPopup) {
            $jQuery.ajax({
                type: "POST",
                url: "/ComplianceOperations/Default.aspx/CheckExistingOrderInformation",
                contentType: "application/json",
                dataType: "json",
                async: false,
                data: JSON.stringify({ pkgID: $jQuery("[id$=hdnSelectedPackageId]").val() }),
                success: function (data) {
                    var res = JSON.parse(data.d);
                    if (res.IsOrderExisted == "true") {
                        // selectedPackageSubcriptionOption.prop('checked', false);
                        //ShowExistingOrderInformation(res.IsSubscriptionRenew);

                        OrderNumber = res.OrderNumber;
                        var popupWindowName = "";
                        var IsCompletePaymentPopup = true;
                        if (res.IsSubscriptionRenew == "false" || res.IsSubscriptionRenew == "0" || !res.IsSubscriptionRenew) {
                            popupWindowName = "Complete your payment";
                        }
                        else {
                            popupWindowName = "Renew your subscription";
                            IsCompletePaymentPopup = false;
                        }

                        //var popupHeight = $jQuery(window).height() * (100 / 100);
                        var popupHeight = 300;

                        var url = $page.url.create("~/ComplianceOperations/Pages/ExistingOrderInformationPage.aspx?IsCompletePaymentPopup=" + IsCompletePaymentPopup + "&OrderNumber=" + OrderNumber);
                        var win = $window.createPopup(url, {
                            size: "600," + popupHeight, behaviors: Telerik.Web.UI.WindowBehaviors.Move, name: popupWindowName, onclose: OnClientClose
                        }
                            , function () {
                                this.set_title(popupWindowName);
                                // this.set_destroyOnClose(true);
                            }
                        );
                        winopen = true;
                        return false;
                    }
                    else {
                        $jQuery('[id$=btnExistingOrderDoPostBack]').click();
                    }
                },
                failure: function (data) {
                    $jQuery('[id$=btnExistingOrderDoPostBack]').click();
                }
            });
        }
        else {
            $jQuery('[id$=btnExistingOrderDoPostBack]').click();
        }
    }
    function OnClientClose(oWnd, args) {
        oWnd.get_contentFrame().src = ''; //This is added for fixing pop-up close issue in Safari browser.
        oWnd.remove_close(OnClientClose);
        if (winopen) {

            var arg = args.get_argument();
            if (arg) {
                var hdnExisitngOrderInformationIgnore = arg.hdnExisitngOrderInformationIgnore;
                $jQuery("[id$=hdnExisitngOrderInformationIgnored]").val(hdnExisitngOrderInformationIgnore[0].value);
            }
            if (args != null && args._argument != null) {
                if ($jQuery("[id$=hdnExisitngOrderInformationIgnored]").val() == 1) {
                    ignoredExistingOrderPackageList.push($jQuery("[id$=hdnSelectedPackageId]").val());
                    $jQuery("[id$=hdnignoredExistingOrderPackageList]").val(JSON.stringify(ignoredExistingOrderPackageList));
                    $jQuery('[id$=btnExistingOrderDoPostBack]').click();
                }
                else {
                    selectedPackageSubcriptionOption.prop('checked', false);
                }
            }

            $jQuery("[id$=hdnPrevPackageId]").val($jQuery("[id$=hdnSelectedPackageId]").val());
            $jQuery("[id$=hdnSelectedPackageId]").val("0");
            winopen = false;
            selectedPackageSubcriptionOption = null;
        }

    }

