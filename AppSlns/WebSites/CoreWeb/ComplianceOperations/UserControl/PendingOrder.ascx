        if (chkLstId != null) {
            var options = $jQuery(".chkBoxList input:checked");
            for (var i = 0; i < options.length; i++) {
                if (options[i].checked) {
                    lblId[0].innerHTML = "";
                    args.set_cancel(false);
                    return true;
                }
            }
            lblId[0].innerHTML = "Program is required.";
            hideShowPendingOrder.hide();
            args.set_cancel(true);
        }
    }

