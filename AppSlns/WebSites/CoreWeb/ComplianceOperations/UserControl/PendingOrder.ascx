    function RegisterbtnSubscriptionsImmSelectedIndexChangedEvent() {
        //rbtnPkgCheckedChanged

        $jQuery('.rbtnPkgCheckedChanged input').click(function () {
            selectedPackageSubcriptionOption = $jQuery(this);
